-- Scripts:

local function VDLO_fake_script() -- HomeB.Tabs 
	local script = Instance.new('LocalScript', HomeB)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.Home.Visible = true
		script.Parent.Parent.Credit.Visible = false
		script.Parent.Parent.Main.Visible = false
		
		wait(0.1)
	end)
end
coroutine.wrap(VDLO_fake_script)()
local function QDXM_fake_script() -- MainB.Tabs 
	local script = Instance.new('LocalScript', MainB)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.Home.Visible = false
		script.Parent.Parent.Credit.Visible = false
		script.Parent.Parent.Main.Visible = true
		
		wait(0.1)
	end)
end
coroutine.wrap(QDXM_fake_script)()
local function OXSGRSU_fake_script() -- CreditB.Tabs 
	local script = Instance.new('LocalScript', CreditB)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.Home.Visible = false
		script.Parent.Parent.Credit.Visible = true
		script.Parent.Parent.Main.Visible = false
		
		wait(0.1)
	end)
end
coroutine.wrap(OXSGRSU_fake_script)()
