RHB.Size = UDim2.new(0, 344, 0, 22)
RHB.ZIndex = 2
RHB.Font = Enum.Font.Highway
RHB.Text = "Remove-Hit-Box"
RHB.TextColor3 = Color3.fromRGB(255, 255, 255)
RHB.TextSize = 17.000
RHB.TextWrapped = true
local function sendNotification(title, text, icon)
    game.StarterGui:SetCore("SendNotification", {
        Title = title;
        Text = text;
        Icon = icon;
    })
end

RHB.MouseButton1Down:Connect(function()
    sendNotification("Notification", "RHB loaded", "7164585376")
    sendNotification("Notification", "Press button after every round", "7164585376")

    local LocalPlayer = game.Players.LocalPlayer

    local function destroyCollisionPart(character)
        pcall(function()
            local collisionPart = character:FindFirstChild("CollisionPart")
            if collisionPart then
                collisionPart:Destroy()
                print("No More Hitbox")
            end
        end)
    end

    -- Destroy CollisionPart in current character
    if LocalPlayer.Character then
        destroyCollisionPart(LocalPlayer.Character)
    end

    -- Connect to CharacterAdded event to handle future characters
    LocalPlayer.CharacterAdded:Connect(function(character)
        for destructionIteration = 1, 100 do
            wait()
            destroyCollisionPart(character)
        end
    end)
end)

